# skip udev extras
sys-fs/udev extras

# dependency is not included in this tree
dev-libs/ppl lpsol

# skip zope dependencies
dev-python/ipython smp

# mask nagios plugins
net-analyzer/nagios-plugins ups nagios-game nagios-ping

# ctdb is not supported yet
net-fs/samba cluster

# disable some imagemagick modules
media-gfx/imagemagick autotrace raw opencl

# do not pull X11 crap for SVG
media-gfx/graphviz cairo

# only neon is supported for now
dev-vcs/subversion webdav-serf

# use pythons builtin datetime
dev-python/psycopg mxdatetime

# do not support SQL for syslog-ng
app-admin/syslog-ng json sql

# do not support opendbx backend
net-dns/pdns opendbx

# virtualbox
app-emulation/virtualbox extensions vboxwebsrv

# collectd
app-admin/collectd collectd_plugins_dbi
app-admin/collectd collectd_plugins_gmond
app-admin/collectd collectd_plugins_ipmi
app-admin/collectd collectd_plugins_libvirt
app-admin/collectd collectd_plugins_notify_desktop
app-admin/collectd collectd_plugins_notify_email
app-admin/collectd collectd_plugins_nut
app-admin/collectd collectd_plugins_onewire
app-admin/collectd collectd_plugins_oracle
app-admin/collectd collectd_plugins_ping
app-admin/collectd collectd_plugins_sensors
app-admin/collectd collectd_plugins_tokyotyrant
app-admin/collectd collectd_plugins_varnish

# do not support X11 with these applications
app-editors/emacs X
app-editors/vim X
app-misc/mc X
app-text/ghostscript-gpl X
dev-java/sun-jdk X
dev-lang/R X
dev-python/imaging X
media-gfx/imagemagick X
media-libs/giflib X
media-libs/t1lib X
net-analyzer/net-snmp X
net-misc/openssh X
sys-apps/groff X
sys-fs/mtools X

# do not support gobject introspection for now
x11-libs/pango introspection
x11-libs/gdk-pixbuf introspection

# do not depend on gdbus-codegen and systemtap
dev-libs/glib utils systemtap

# do not depend on python-3.3
sys-apps/portage xattr

# skip libssh2
net-misc/curl ssh
