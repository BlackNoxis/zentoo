# skip udev extras
sys-fs/udev extras

# dependency is not included in this tree
dev-libs/ppl lpsol

# skip zope dependencies
dev-python/ipython smp

# mask nagios plugins
net-analyzer/nagios-plugins ups nagios-game nagios-ping

# ctdb is not supported yet
net-fs/samba cluster

# disable some imagemagick modules
media-gfx/imagemagick autotrace raw opencl

# do not pull X11 crap for SVG
media-gfx/graphviz cairo

# only neon is supported for now
dev-vcs/subversion webdav-serf

# use pythons builtin datetime
dev-python/psycopg mxdatetime

# do not support SQL for syslog-ng
app-admin/syslog-ng sql

# do not support opendbx backend
net-dns/pdns opendbx

# do not support c-ares
net-misc/curl ares
